                                    "jumlah_halaman": {
                                        "type": "integer",
                                        "description": "Number of pages",
                                        "nullable": true
                                    },
                                    "isbn": {
                                        "type": "string",
                                        "maxLength": 13,
                                        "description": "ISBN number of the book"
                                    },
                                    "kategori": {
                                        "type": "array",
                                        "items": {
                                            "type": "integer",
                                            "description": "Category ID"
